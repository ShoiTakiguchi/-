<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee web-app_2_4.xsd"
	version="2.4">

	<filter>
		<filter-name>LoginFilter</filter-name>
		<filter-class>servlet.LoginFilter</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>LoginFilter</filter-name>
		<url-pattern>/jsp/user/confirmOrder.jsp</url-pattern>
	</filter-mapping>
	<filter-mapping>
		<filter-name>LoginFilter</filter-name>
		<url-pattern>/jsp/user/orderRegist.jsp</url-pattern>
	</filter-mapping>
	<filter-mapping>
		<filter-name>LoginFilter</filter-name>
		<url-pattern>/jsp/order/orderSearching.jsp</url-pattern>
	</filter-mapping>
	<filter-mapping>
		<filter-name>LoginFilter</filter-name>
		<url-pattern>/jsp/order/orderReading.jsp</url-pattern>
	</filter-mapping>
	<filter-mapping>
		<filter-name>LoginFilter</filter-name>
		<url-pattern>/jsp/user/studentMypage.jsp</url-pattern>
	</filter-mapping>
	<filter-mapping>
		<filter-name>LoginFilter</filter-name>
		<url-pattern>/jsp/user/managerMypage.jsp</url-pattern>
	</filter-mapping>



	<servlet>
		<servlet-name>LoginServlet</servlet-name>
		<servlet-class>servlet.LoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/jsp/user/LoginServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>LogoutServlet</servlet-name>
		<servlet-class>servlet.LogoutServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LogoutServlet</servlet-name>
		<url-pattern>/jsp/user/LogoutServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>ConfirmOrderServlet</servlet-name>
		<servlet-class>servlet.ConfirmOrderServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ConfirmOrderServlet</servlet-name>
		<url-pattern>/jsp/ConfirmOrderServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>RegistOrderServlet</servlet-name>
		<servlet-class>servlet.RegistOrderServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>RegistOrderServlet</servlet-name>
		<url-pattern>/jsp/RegistOrderServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>ReadOrderServlet</servlet-name>
		<servlet-class>servlet.ReadOrderServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ReadOrderServlet</servlet-name>
		<url-pattern>/jsp/order/ReadOrderServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>SearchOrderServlet</servlet-name>
		<servlet-class>servlet.SearchOrderServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SearchOrderServlet</servlet-name>
		<url-pattern>/jsp/order/SearchOrderServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>GoMypageServlet</servlet-name>
		<servlet-class>servlet.GoMypageServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GoMypageServlet</servlet-name>
		<url-pattern>/jsp/user/GoMypageServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>MailSettingServlet</servlet-name>
		<servlet-class>servlet.MailSettingServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>MailSettingServlet</servlet-name>
		<url-pattern>/jsp/user/MailSettingServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>ChangeFlagServlet</servlet-name>
		<servlet-class>servlet.ChangeFlagServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ChangeFlagServlet</servlet-name>
		<url-pattern>/jsp/order/ChangeFlagServlet</url-pattern>
	</servlet-mapping>



</web-app>